    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    tools:context="unilever.coachingform.SRPullReportActivity">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="10dp">
        <LinearLayout
            android:id="@+id/sub_page_option"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="20dp">
            <Button
                android:id="@+id/report_coaching"
                android:text="@string/title_bahasa_rspull_report"
                style="@style/sub_page_coaching" />
            <Button
                android:id="@+id/on_coaching"
                android:text="@string/title_bahasa_rspull_kunjungan"
                style="@style/sub_page_coaching"
                android:background="@color/gray"/>
        </LinearLayout>
        <TextView
            android:id="@+id/description"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/srpull_description"
            android:layout_below="@+id/sub_page_option"
            android:gravity="center_horizontal"
            style="@style/description_style"
            android:layout_margin="5dp"/>
        <View
            android:id="@+id/line1"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/orange"
            android:layout_below="@+id/description"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"/>
        <TableLayout
            android:id="@+id/profile_line"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:stretchColumns="*"
            android:layout_margin="10dp"
            android:layout_below="@+id/line1">
            <TableRow
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <TextView
                    android:text="Coach"
                    android:textSize="15dp"
                    android:layout_weight="0.5"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" />
                <EditText
                    android:id="@+id/coach"
                    style="@style/table_edittext_type"
                    android:layout_marginLeft="10dp"
                    android:hint="Coach"
                    android:layout_weight="0.5"/>
            </TableRow>
            <TableRow
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <TextView
                    android:text="Coachee"
                    android:textSize="15dp"
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" />
                <EditText
                    android:id="@+id/coachee"
                    style="@style/table_edittext_type"
                    android:hint="Coachee"
                    android:layout_marginLeft="10dp"
                    android:layout_weight="1"/>
            </TableRow>
            <TableRow
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <TextView
                    android:text="Area"
                    android:textSize="15dp"
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" />
                <EditText
                    android:id="@+id/area"
                    style="@style/table_edittext_type"
                    android:hint="Area"
                    android:layout_marginLeft="10dp"
                    android:layout_weight="1"/>
            </TableRow>
        </TableLayout>
