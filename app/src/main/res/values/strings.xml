    <string name="title_bahasa_dtspull_pasar_kunjungan">MARKET</string>
    <string name="title_english_dtspull_sebelum_kunjungan">BEFORE ARRIVING TO DIST</string>
    <string name="title_english_dtspull_saat_report_kunjungan">IN DISTRIBUTOR\n(REPORT)</string>
    <string name="title_english_dtspull_saat_infra_kunjungan">IN DISTRIBUTOR\n(INFRASTRUCTURE)</string>
